<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Commercial" table="commercial">
    <id name="comId" type="integer" column="com_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="comName" type="string" column="com_name" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="comMail" type="string" column="com_mail" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="comPassword" type="string" column="com_password" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
